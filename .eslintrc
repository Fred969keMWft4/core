{
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-cond-assign": [2, "always"],
    "no-extra-parens": 0,
    "no-reserved-keys": 2,
    "no-unexpected-multiline": 2,

    "accessor-pairs": 2,
    "complexity": [2, 5],
    "consistent-return": 0,
    "default-case": 2,
    "dot-location": [2, "property"],
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 2,
    "no-div-regex": 2,
    "no-else-return": 2,
    "no-floating-decimal": 2,
    "no-multi-spaces": [2, {"exceptions": {"Property": false}}],
    "no-param-reassign": 2,
    "no-return-assign": [2, "always"],
    "no-self-compare": 2,
    "no-throw-literal": 2,
    "no-void": 2,
    "no-warning-comments": [2, {"location": "anywhere"}],
    "radix": 2,
    "wrap-iife": 2,

    "strict": [2, "function"],

    "no-undefined": 2,

    "handle-callback-err": 2,
    "no-new-require": 2,

    "array-bracket-spacing": 2,
    "brace-style": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "consistent-this": [2, "that"],
    "func-names": 2,
    "func-style": [0, "declaration"],
    "indent": [2, 2],
    "lines-around-comment": [0, {"beforeLineComment": true}],
    "linebreak-style": [0, "unix"],
    "max-nested-callbacks": [2, 3],
    "newline-after-var": 2,
    "no-continue": 2,
    "no-inline-comments": 2,
    "no-lonely-if": 2,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-nested-ternary": 0,
    "no-ternary": 0,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": 2,
    "one-var": [0, {"let": "never", "const": "never"}],
    "operator-assignment": [2, "always"],
    "operator-linebreak": 2,
    "padded-blocks": [2, "never"],
    "quote-props": [2, "as-needed"],
    "quotes": [2, "single"],
    "space-after-keywords": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-in-parens": 2,
    "space-unary-ops": 2,
    "spaced-comment": 0,

    "constructor-super": 2,
    "generator-star-spacing": [2, "after"],
    "no-this-before-super": 2,
    "no-var": 2,
    "object-shorthand": 0,
    "prefer-const": 0,

    "max-depth": [2, 3],
    "max-len": [0, 80, 2],
    "max-params": [2, 3],
    "max-statements": [2, 30],
    "no-bitwise": 2,
    "no-plusplus": 2
  }
}
